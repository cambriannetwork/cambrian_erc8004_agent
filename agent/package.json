    "validation": "node scripts/create-validation.js",
    "query": "node scripts/query-complete-state.js",
    "validate-flow": "node scripts/demonstrate-validated-flow.js",
    "subgraph:codegen": "cd subgraph && graph codegen",
    "subgraph:build": "cd subgraph && graph build",
    "subgraph:deploy": "cd subgraph && graph deploy erc-8004 --version-label v2.0.2 --deploy-key 1c09c30fff4eb37d01899da57c4c3293 --node https://api.studio.thegraph.com/deploy",
    "contract:compile": "npx solc --abi --bin contracts/ERC8004Complete.sol -o build",
    "clean": "rm -rf build cache artifacts typechain-types"
  },
