{
  "name": "erc8004-cambrian-defi-data",
  "version": "1.0.0",
  "description": "Production-ready ERC-8004 DeFi Data Agent serving real-time Solana token prices from Cambrian Network",
  "main": "cambrian-defi-data-agent.js",
  "scripts": {
    "start": "node cambrian-defi-data-agent.js",
    "seller": "node cambrian-defi-data-agent.js",
    "deploy": "node deploy-contracts.js",
    "demo": "node run-demo.js",
    "cambrian": "node cambrian-defi-data-agent.js",
    "test": "node scripts/query-complete-state.js",
    "test:agents": "node test-agent-entity.js",
    "test:services": "node test-serviceexecution-entity.js",
    "test:feedback": "node test-feedbackv2-entity.js",
    "test:validation": "node test-validationrequest-entity.js",
    "test:deployed": "node test-deployed-services.js",
